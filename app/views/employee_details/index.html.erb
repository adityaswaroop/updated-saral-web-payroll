<h1>Listing employee_details</h1>

<table>
  <tr>
    <th>Employee</th>
    <th>Effective date</th>
    <th>Salary group</th>
    <th>Allotted gross</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @employee_details.each do |employee_detail| %>
  <tr>
    <td><%= employee_detail.employee_id %></td>
    <td><%= employee_detail.effective_date %></td>
    <td><%= employee_detail.salary_group_id %></td>
    <td><%= employee_detail.allotted_gross %></td>
    <td><%= link_to 'Show', employee_detail %></td>
    <td><%= link_to 'Edit', edit_employee_detail_path(employee_detail) %></td>
    <td><%= link_to 'Destroy', employee_detail, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

