#left_content
  = render 'left'
#right_content
  %h1
    Showing PF Group Rate
    = link_to image_tag('/assets/edit.png',:title => "Edit PF Group Rate"), edit_pf_group_rate_path(@pf_group_rate)
  %table.std_table{:cellpadding=>0,:cellspacing=>0}
    %tr
      %td
        %table
          %tr
            %td.std_td
              %table
                %tr
                  %td Pf group:&nbsp;
                  %td= @pf_group_rate.pf_group_id
                %tr
                  %td Paymonth:&nbsp;
                  %td= @pf_group_rate.paymonth_id
                %tr
                  %td Account number 21:&nbsp;
                  %td= @pf_group_rate.account_number_21