#left_content
  = render 'left'
#right_content
  %h1
    Showing HR Category Detail
    = link_to image_tag('/assets/edit.png',:title => "Edit HR Category Detail"), edit_hr_category_detail_path(@hr_category_detail)
  %table.std_table{:cellpadding=>0,:cellspacing=>0}
    %tr
      %td
        %table
          %tr
            %td.std_td
              %table
                %tr
                  %td{:width=>130} HR Category :&nbsp;
                  %td= @hr_category_detail.hr_category.category_name
                %tr
                  %td{:width=>130} Name :&nbsp;
                  %td= @hr_category_detail.name
          %tr
            %td.std_td
              %table
                %tr
                  %td{:width=>130} Data Type :&nbsp;
                  %td= @hr_category_detail.data_type
                %tr
                  %td{:width=>130} Required :&nbsp;
                  %td= @hr_category_detail.required
          %tr
            %td &nbsp;
